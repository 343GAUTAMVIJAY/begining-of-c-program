#include<stdio.h>
int main()
{
  int n1,n2,n3;
  scanf("%d%d%d",&n1,&n2,&n3);
  if(n1>n2 && n1>n3)
  
      printf("%d is biggest number",n1);
  }
  else if(n2>n1 && n2>n3)
  {
      printf("%d is biggest number",n2);
  }
  else 
  {
      printf("%d is biggest number",n3); 
  }
}

**************************************************************
Output
2 4 6
6 is biggest number
